{
  "name": "saga-player",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.16.0",
    "@reduxjs/toolkit": "^1.8.2",
    "antd": "^4.21.3",
    "axios": "^0.27.2",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "dayjs": "^1.11.3",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "fs-extra": "^10.0.0",
    "identity-obj-proxy": "^3.0.0",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.18.0",
    "react-redux": "^8.0.2",
    "react-refresh": "^0.11.0",
    "react-router-dom": "^6.3.0",
    "resolve": "^1.20.0",
    "semver": "^7.3.5"
  },
  "scripts": {
    "dev": "concurrently \"pnpm start\" \"wait-on http://localhost:3000 && pnpm run ele\"",
    "build": "npx cross-env GENERATE_SOURCEMAP=false node scripts/build.js",
    "ele": "nodemon --watch ./main.js --exec \"electron .\"",
    "start": "node scripts/start.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.2.1",
